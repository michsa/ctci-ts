{
  "name": "ctci-ts",
  "version": "1.0.0",
  "description": "Solutions from _Cracking the Coding Interview_ implemented in Typescript, for testing with Mocha/Chai",
  "main": "index.js",
  "scripts": {
    "test": "npm run build-test && mocha test",
    "lint": "tslint -c tslint.json -p tsconfig-base.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig-base.json --fix",
    "build-dist": "tsc -p tsconfig-src.json",
    "build-test": "tsc -p tsconfig-test.json"
  },
  "author": "Michelle Saad",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^6.0.107",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "npm": "^6.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  }
}
